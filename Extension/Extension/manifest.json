{
    "manifest_version": 2,

    "name": "Phishing Detector",
    "description":" An educational tool that will give you tips on what to look for in a URL and prevent you from phishing attacks. ",
    "version": "1.4",
    "permissions":["storage", "tabs", "activeTab", "background","<all_urls>"],
    "background": {
        "scripts": ["JS/background.js", "JS/jquery.js"],
        "persistent": false
    },
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js":["JS/jquery.js", "JS/content.js","JS/popup.js"]
        }
    ],

    "browser_action": {
        "default_title":"ChangeME",
        "default_icon":  "PNG/default_icon.png",
	    "default_popup": "HTML/popup.html"
        
    }
}